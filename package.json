{
  "name": "moris",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
